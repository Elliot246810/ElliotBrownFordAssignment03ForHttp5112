@model IEnumerable<ElliotBrownFordAssignment03ForHttp5112.Models.Student>
@{
    ViewBag.Title = "List";
}


<div id="containerlist">
    <h2>List Of Student</h2>
    <form action="/Student/List" method="GET">
        <input type="text" name="searchKeyForNames" />
        <input type="submit" value="Search" />
    </form>
    @foreach (var student in Model)
    {
        <div class="listitem row">
            <div class="col-md-12"><a href="/Student/Show/@student.StudentId">@student.StudentFname @student.StudentLname</a></div>

        </div>
    }

</div>